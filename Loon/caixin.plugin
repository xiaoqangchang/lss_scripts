#!name = 财新去广告
#!desc = 移除财新APP内的广告和推广内容
#!author = Claude
#!homepage = https://github.com
#!icon = https://is1-ssl.mzstatic.com/image/thumb/PurpleSource221/v4/62/4e/ad/624ead29-a41a-1de3-551f-06aa893e8d32/Placeholder.mill/400x400bb-75.webp
#!date = 2026-01-29 19:59:46

[Rule]
DOMAIN, dolphin.caixin.com, REJECT
DOMAIN, gg.caixin.com, REJECT

[Rewrite]
# 拦截股票数据推广
^https:\/\/cxdata\.caixin\.com\/api\/dataplus\/stkData\/list reject-dict

# 拦截首页推送广告
^https:\/\/entities\.caixin\.com\/api\/public\/push\/appIndex\?target=index reject-dict

# 拦截搜索关键词推荐
^https:\/\/gateway\.caixin\.com\/api\/app-api\/search\/getkeyword reject-dict

# 拦截小程序信息
^https:\/\/gateway\.caixin\.com\/api\/app-api\/caixinminiapp\/appinfo reject-dict

# 拦截热门推荐文章
^https:\/\/gateway\.caixin\.com\/api\/app-api\/articlesHotRecommend\/getHotRecommendArticles reject-dict

# 拦截AI问答推广
^https:\/\/www\.caixin\.com\/ai\/question.+ reject-dict

[MITM]
hostname = cxdata.caixin.com, entities.caixin.com, gateway.caixin.com, www.caixin.com

